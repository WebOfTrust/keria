{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
